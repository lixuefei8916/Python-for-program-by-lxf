print u"输入a,b,c三个值，输出其最大者；"

a = raw_input("Please input a: ")
b = raw_input("Please input b: ")
c = raw_input("Please input c: ")

if a>b:
	if a>c:
		print a
	else:
		print c
else:
	if b>c:
		print b
	else:
		print c

#=========================================
print u"2.	求 1x2x3x4x5"
p = 1   	#乘数
i = 2 		#被乘数
for i in range(1,6):
	x = p
	p = p*i
	print "%s * %s = %s" %(x,i,p)
	i = i+1
print p

